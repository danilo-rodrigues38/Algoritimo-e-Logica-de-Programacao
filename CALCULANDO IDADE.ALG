algoritmo "Calculando idade"
var
   dtatual, dtnasc, idade: inteiro
inicio
   Escreva ("Entre com o ano atual: ")
   Leia (dtatual)
   Escreva ("Entre como ano de nascimento: ")
   Leia (dtnasc)
   idade <- dtatual - dtnasc
   Escreval ("Voçê tem", idade, " anos.")
   Escreval ("Parabéns!!!")



fimalgoritmo